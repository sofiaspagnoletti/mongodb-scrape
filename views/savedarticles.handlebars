<div id="home">
  <div id="wrapper">
    <h1>News!</h1>
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
      <a class="navbar-brand" href="/">Mongo Scraper</a>
      <button class="navbar-toggler" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01"
        aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-link" href="/">Home
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/savedarticles.handlebars">Saved Articles</a>
            <span class="sr-only">(current)</span>
          </li>
          <li class="nav-item">
            <a class="btn btn-danger scrape-new">SCRAPE NEW ARTICLES!</a>
          </li>
          <li class="nav-item">
            <a class="btn btn-danger clear">CLEAR ARTICLES!</a>
          </li>
        </ul>
      </div>

      <div class="container-fluid article-container">
        {{#each Articles}}
        <p>{{headline}}</p>
        <p>{{summary}}</p>
        <p>{{url}}</p>
        {{/each}}
      </div>
    </nav>

  </div>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="app.js"></script>